[
    // editor navigation
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "cmd+shift+[",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "cmd+shift+]",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+h",
        "command": "workbench.action.focusAuxiliaryBar"
    },
    {
        "key": "ctrl+shift+h",
        "command": "workbench.action.toggleAuxiliaryBar",
        "when": "auxiliaryBarFocus"
    },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.focusPanel"
    },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.togglePanel",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+shift+k",
        "command": "workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "explorerViewletFocus"
    },
    // pane navigation
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLeftGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusBelowGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusAboveGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLeftGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusBelowGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusAboveGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLeftGroup",
        "when": "explorerviewletfocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusBelowGroup",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusAboveGroup",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup",
        "when": "explorerViewletFocus"
    },
    // editor actions
    {
        "key": "cmd+/",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+backspace",
        "command": "editor.action.deleteLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+c",
        "command": "editor.action.clipboardCopyAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+ctrl+z",
        "command": "git.revertSelectedRanges",
        "when": "editorTextFocus && !editorReadonly"
    },
    // multicursor
    {
        "key": "cmd+shift+k",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+shift+j",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus && !editorReadonly"
    },
    // editor splittingc
    {
        "key": "ctrl+d",
        "command": "workbench.action.splitEditorRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.action.splitEditorDown",
        "when": "editorTextFocus"
    },
    // find and replace
    {
        "key": "cmd+r",
        "command": "editor.action.startFindReplaceAction",
        "when": "editorTextFocus"
    },
    // scrolling
    {
        "key": "cmd+ctrl+shift+j",
        "command": "editorScroll",
        "args": {
            "to": "down"
        },
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+ctrl+shift+k",
        "command": "editorScroll",
        "args": {
            "to": "up"
        },
        "when": "editorTextFocus"
    },
    {
        "key": "shift+j",
        "command": "editorScroll",
        "args": {
            "to": "down"
        },
        "when": "explorerViewletFocus"
    },
    {
        "key": "shift+k",
        "command": "editorScroll",
        "args": {
            "to": "up"
        },
        "when": "explorerViewletFocus"
    },
    // zoom
    {
        "key": "ctrl+-",
        "command": "editor.action.fontZoomOut",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+=",
        "command": "editor.action.fontZoomIn",
        "when": "editorTextFocus"
    },
    // suggestions and quick fixes
    {
        "key": "cmd+j",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
    },
    {
        "key": "cmd+k",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
    },
    {
        "key": "cmd+j",
        "command": "list.focusDown",
        "when": "editorTextFocus && quickFixWidgetVisible"
    },
    {
        "key": "cmd+k",
        "command": "list.focusUp",
        "when": "editorTextFocus && quickFixWidgetVisible"
    },
    // list navigation
    {
        "key": "cmd+j",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "cmd+k",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    // file explorer
    {
        "key": "d",
        "command": "explorer.newFolder",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "f",
        "command": "explorer.newFile",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "cmd+shift+c",
        "command": "copyRelativeFilePath",
        "when": "editorTextFocus"
    },
    // terminal
    {
        "key": "cmd+`",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "cmd+k",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
    },
    // git
    {
        "key": "cmd+shift+g",
        "command": "workbench.view.scm"
    },
    // go to reference/implementation
    {
        "key": "cmd+j",
        "command": "editor.action.goToReferences",
        "when": "editorHasReferenceProvider"
    },
    // recent files
    {
        "key": "cmd+o",
        "command": "workbench.action.openRecent"
    },
    // remove defaults
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+j",
        "command": "-workbench.action.togglePanel"
    },
    {
        "key": "cmd+l",
        "command": "-composer.sendToAgent",
        "when": "editorHasPromptBar && editorPromptBarFocused"
    },
    {
        "key": "cmd+l",
        "command": "-aichat.newchataction"
    },
    {
        "key": "shift+cmd+h",
        "command": "aichat.insertselectionintochat"
    },
    {
        "key": "shift+cmd+l",
        "command": "-aichat.insertselectionintochat"
    },
    {
        "key": "ctrl+cmd+h",
        "command": "addCursorsAtSearchResults",
        "when": "fileMatchOrMatchFocus && searchViewletVisible"
    },
    {
        "key": "shift+cmd+l",
        "command": "-addCursorsAtSearchResults",
        "when": "fileMatchOrMatchFocus && searchViewletVisible"
    },
    {
        "key": "shift+cmd+h",
        "command": "aichat.newchataction"
    },
    {
        "key": "shift+cmd+o",
        "command": "workbench.action.files.openFileFolder",
        "when": "isMacNative && openFolderWorkspaceSupport"
    },
    {
        "key": "cmd+o",
        "command": "-workbench.action.files.openFileFolder",
        "when": "isMacNative && openFolderWorkspaceSupport"
    },
    {
        "key": "shift+cmd+o",
        "command": "workbench.action.files.openFile",
        "when": "false"
    },
    {
        "key": "cmd+o",
        "command": "-workbench.action.files.openFile",
        "when": "false"
    },
    {
        "key": "shift+cmd+o",
        "command": "workbench.action.files.openFolderViaWorkspace",
        "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
    },
    {
        "key": "cmd+o",
        "command": "-workbench.action.files.openFolderViaWorkspace",
        "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
    },
    {
        // TODO: find a shortcut for this (?)
        "key": "cmd+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "cmd+t",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "cmd+p",
        "command": "-workbench.action.quickOpen"
    },
]